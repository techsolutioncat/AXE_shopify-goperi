@font-face {
  font-family: 'RocGrotesk-Regular';
  src:  url('{{ 'roc-regular.woff' | asset_url }}') format('woff'),
        url('{{ 'RocGrotesk-Regular.ttf' | asset_url }}') format('truetype');
  
}

@font-face {
  font-family: 'RocGrotesk-Medium';
  src:  url('{{ 'roc-medium.woff' | asset_url }}') format('woff'),
        url('{{ 'RocGrotesk-Medium.ttf' | asset_url }}') format('truetype');
}
@font-face {
  font-family: 'RocGrotesk-Bold';
  src:  url('{{ 'roc-bold.woff' | asset_url }}') format('woff'),
        url('{{ 'RocGrotesk-Bold.ttf' | asset_url }}') format('truetype');
}

@font-face {
  font-family: 'Acumin Pro';
  src: url('Acumin-R.woff') format('woff');
}
@font-face {
  font-family: 'Acumin Italic';
  src: url('Acumin-It.woff') format('woff');
}
@font-face {
  font-family: 'Acumin Bold';
  src: url('Acumin-Bd.woff') format('woff');
}
@font-face {
  font-family: 'Acumin Bold Italic';
  src: url('Acumin-BdIt.woff') format('woff');
}
